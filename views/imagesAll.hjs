<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>All years</h1>
    <ul>
    {{#years}}
      <li>
        <a href="/images/{{.}}">{{.}}</a>
      </li>
    {{/years}}
    {{#months}}
      <li>
        <a href="/images/{{year}}/{{.}}">{{.}}</a>
      </li>
    {{/months}}
    </ul>
    {{#images}}
      {{day}}
      <ul>
        {{#dataImages}}
          <li>
            <img width="640" src="/imageBinary?path={{.}}" />
          </li>
        {{/dataImages}}
      </ul>
    {{/images}}
  </body>
</html>